<h1>Edit Iteration Detail</h1>

<%= form_for @iteration, :url =>{:action => "update", :id =>@iteration} do |f| %>

<p>Name: <%= f.text_field 'name' %></p>
<p>Start Date: <%= f.date_field  'start_date' %></p>
<p>End Date: <%= f.date_field  'end_date' %></p>
<%= f.submit "Save changes" %>
<% end %>

<%= link_to 'Back', {:action => 'list' } %>